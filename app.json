{
  "expo": {
    "scheme" : "myapp",
    "name": "Student Loan",
    "slug": "student-loan-app-client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package" : "com.company.studentloan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "androidStatusBar": {
      "backgroundColor": "#00BFA6",
      "translucent": false,
      "barStyle":"light-content"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": ".local/assets/logo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/logo.png",
      "color": "#311b92",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Colab.notes"
    }
  }
}
